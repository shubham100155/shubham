#include<stdio.h>


int main()
{
  printf("Name\t\t:\t shubham patil\n");
  printf("Age\t\t\t:\t19\n");
  printf("Class\t:\tFirst Year\n");
  printf("Div\t\t\t:\tC3\n");
  printf("UIN\t\t\t:\t241P051\n");


  return 0;
}
